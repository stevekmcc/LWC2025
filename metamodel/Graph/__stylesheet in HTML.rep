__stylesheet\ in\ HTML(@dir)
variable 'writtenStylesheet' append close
if not $writtenStylesheet then
	filename @dir 'metaedit.css' write
		subreport '__stylesheet data' run
	close 
	$writtenStylesheet='T'
endif
@qlsDir=__(if @dir=~__('*'sep 'QLS' sep) then '..' sep endif)
'
<title>' id%xml ' report</title>
<link rel="stylesheet" type="text/css" href="' @qlsDir 'metaedit.css" />
'
